  background-color: var(--tg-shopify-design-system---base-color-neutral--neutral-lightest);
  object-fit: contain;
  width: 5rem;
  height: 5rem;
}

.tg-shopify-design-system--mini-cart_full {
  flex-direction: column;
  flex: 1;
  justify-content: space-between;
  max-height: 100vh;
  display: flex;
}

.tg-shopify-design-system--mini-cart_empty-content {
  flex: 1;
  justify-content: center;
  align-items: center;
  height: 100%;
  display: flex;
}

.tg-shopify-design-system--mini-cart_line-item_quantity {
  border-style: solid;
  border-width: 1px;
  border-color: var(--tg-shopify-design-system---base-color-neutral--black) black black;
  justify-content: space-between;
  display: flex;
}

.tg-shopify-design-system--mini-cart_line-item_quantity-adjust {
  text-align: center;
  border: 1px #000;
  justify-content: center;
  align-items: center;
  width: 2rem;
  height: 2rem;
  display: flex;
}

.tg-shopify-design-system--mini-cart_line-item_product-info {
  grid-column-gap: .5rem;
  grid-row-gap: .5rem;
  flex-direction: column;
  flex: 1;
  align-items: flex-start;
  display: flex;
}

.tg-shopify-design-system--mini-cart_footer-total {
  justify-content: space-between;
  align-items: flex-end;
  display: flex;
}

.tg-shopify-design-system--mini-cart_line-item_price {
  grid-column-gap: .5rem;
  grid-row-gap: .5rem;
  display: flex;
}

.tg-shopify-design-system--nav_shop-icon {
  cursor: pointer;
  justify-content: center;
  align-items: center;
  width: 2rem;
  height: 2rem;
  padding: 0;
  display: flex;
}

.tg-shopify-design-system--icon-embed-xsmall {
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 1.5rem;
  height: 1.5rem;
  display: flex;
}

.tg-shopify-design-system--nav_shop-quantity {
  background-color: var(--tg-shopify-design-system---base-color-neutral--black);
  color: var(--tg-shopify-design-system---base-color-neutral--white);
  border-radius: 100%;
  justify-content: center;
  align-items: center;
  width: 1rem;
  height: 1rem;
  font-size: .7rem;
  display: flex;
  position: absolute;
  inset: auto 0% 0% auto;
