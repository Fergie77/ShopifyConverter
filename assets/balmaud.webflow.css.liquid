  font-weight: 600;
  line-height: 1.2;
}

.tg-shopify-design-system--text-size-tiny {
  font-size: .75rem;
}

.tg-shopify-design-system--text-size-tiny.tg-shopify-design-system--text-color-grey.tg-shopify-design-system--text-style-link {
  cursor: pointer;
}

.tg-shopify-design-system--text-size-regular {
  font-size: 1rem;
}

.tg-shopify-design-system--text-weight-bold {
  font-weight: 700;
}

.tg-shopify-design-system--text-style-muted {
  opacity: .6;
}

.tg-shopify-design-system--text-size-small {
  font-size: .875rem;
}

.tg-shopify-design-system--text-style-link {
  color: #000;
  text-decoration: underline;
}

.tg-shopify-design-system--button {
  border: 1px solid var(--tg-shopify-design-system---base-color-neutral--black);
  background-color: var(--tg-shopify-design-system---base-color-neutral--black);
  color: var(--tg-shopify-design-system---text-color--text-alternate);
  text-align: center;
  white-space: nowrap;
  padding: .65rem 1.5rem;
  font-weight: 400;
}

.tg-shopify-design-system--button.tg-shopify-design-system--is-secondary {
  border: 1px solid var(--tg-shopify-design-system---base-color-neutral--black);
  color: #000;
  background-color: #0000;
}

.tg-shopify-design-system--hide {
  flex: 1;
  display: none;
}

.tg-shopify-design-system--mini-cart_component {
  background-color: var(--tg-shopify-design-system---base-color-neutral--white);
  flex-direction: column;
  align-items: stretch;
  width: 28rem;
  height: 100vh;
  max-height: 100vh;
  display: flex;
}

.tg-shopify-design-system--mini-cart_header {
  border-bottom: 1px solid var(--tg-shopify-design-system---base-color-neutral--neutral-lightest);
  justify-content: space-between;
  align-items: center;
  padding: 1.5rem;
  display: flex;
}

.tg-shopify-design-system--mini-cart_footer {
  grid-column-gap: 1rem;
  grid-row-gap: 1rem;
