<div li-block="Add to Cart">
                    <div class="product-header_form-block w-form">
                      {% form 'product', product, class: 'product-header_default-state' %}
                        {% if product.has_only_default_variant != true %}<div li-if="product.has_only_default_variant != true" class="product-header_option-list">
                          <div li-element="product-options-loop" class="product-header_option">
                            <div li-element="product-options-name">Variant</div>
                            <div class="product-header_radio-list"><label li-element="product-option-loop" class="product-header_radio w-radio">
                                <div class="w-form-formradioinput w-form-formradioinput--inputType-custom product-header_radio-button w-radio-input"></div><input type="radio" name="Radio" id="radio-6" data-name="Radio 6" li-element="product-option-input" style="opacity:0;position:absolute;z-index:-1" value="Radio"><span li-element="product-option-name" class="text-size-regular text-weight-semibold w-form-label" for="radio-6">XS</span>
                              </label></div>
                          </div>
                        </div>{% endif %}
                        <div class="product-header_add-to-cart-wrapper"><input class="form_input w-input" maxlength="256" name="field" data-name="Field" placeholder="Note" li-attribute:name="properties[Note]" type="text" id="field">
                          <template x-if="selected_or_first_available_variant.available">
    <div li-js-if="selected_or_first_available_variant.available" class="product-header_buy-wrappper" >
        <input class="form_input is-quantity w-input" maxlength="256" name="quantity-4" li-element="add-to-cart-quantity" data-name="Quantity 4" placeholder="1" type="number" id="quantity-4" required="" x-model="quantity">
                            <a li-element="add-to-cart" href="#" class="button max-width-full w-button" @click="addToCart($event, true)" :disabled="!addToCartButton">Add to cart</a>
                          
            </div>
    </template>
                          <template x-if="!selected_or_first_available_variant.available">
    <div li-js-if="!selected_or_first_available_variant.available" class="product-header_out-of-stock" >
        
                            <a href="#" class="button is-secondary max-width-full pointer-events-off w-button">Not in Stock</a>
                          
            </div>
    </template>
                        </div>
                      {% endform %}
                      <div class="hide w-form-done">
                        <div>Thank you! Your submission has been received!</div>
                      </div>
                      <div class="hide w-form-fail">
                        <div>Oops! Something went wrong while submitting the form.</div>
                      </div>
                    </div>
                  </div> {% schema %} {
    "name": "Add to Cart",
    "settings": [],
    "presets": [
        {
            "name": "Add to Cart"
        }
    ]
} {% endschema %}